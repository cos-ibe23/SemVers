meta {
  name: Get Pickup
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/v1/pickups/:id
  body: none
  auth: bearer
}

params:path {
  id: 1
}

auth:bearer {
  token: {{shipperToken}}
}

docs {
  Get a specific pickup by ID.

  Path Parameters:
  - id: The pickup ID

  Returns the pickup object with all details.
  Returns 404 if not found or doesn't belong to the shipper.
}
