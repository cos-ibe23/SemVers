meta {
  name: Get Me
  type: http
  seq: 4
}

get {
  url: {{baseUrl}}/v1/auth/me
  body: none
  auth: bearer
}

auth:bearer {
  token: {{shipperToken}}
}

docs {
  Get the current authenticated user with full profile.
  
  Returns:
  - user: Full user object including business fields
  - session: Current session info
  
  Use this after sign-in to get complete user data.
}
