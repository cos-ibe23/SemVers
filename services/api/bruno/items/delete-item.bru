meta {
  name: Delete Item
  type: http
  seq: 5
}

delete {
  url: {{baseUrl}}/v1/items/:id
  body: none
  auth: bearer
}

params:path {
  id: 1
}

auth:bearer {
  token: {{shipperToken}}
}

docs {
  Delete an item from a pickup.

  Path Parameters:
  - id: The item ID

  Returns { success: true } on success.
  Returns 404 if not found or doesn't belong to a shipper's pickup.
}
